{
    "include": ["runtime.gen.mjs"],
    "compilerOptions": { 
        "target": "esnext",
        "module": "ES2015",
        "jsx": "react",
        "inlineSourceMap": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "noImplicitAny": true,
        "allowJs": true,
        "composite": true,
        "outDir": "./out",
        "typeRoots": [
            "node_modules",
            "node_modules/@types",
        ],
    }
}